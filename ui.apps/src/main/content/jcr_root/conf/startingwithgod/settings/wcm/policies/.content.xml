<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="cq:Page">
    <rep:policy/>
    <startingwithgod
        jcr:primaryType="nt:unstructured"
        sling:resourceType="nt:unstructured">
        <core
            jcr:primaryType="nt:unstructured"
            sling:resourceType="nt:unstructured">
            <components
                components="[group:Cru.Core]"
                jcr:primaryType="nt:unstructured"
                jcr:title="Startingwithgod Core Components"
                sling:resourceType="wcm/core/components/policy/policy">
                <jcr:content
                    jcr:primaryType="nt:unstructured"
                    mergeList="{Boolean}true"/>
            </components>
            <experience-fragments
                    components="[group:Cru.Core,/libs/cq/experience-fragments/editor/components/experiencefragment,/libs/cq/experience-fragments/editor/components/buildingblock,/libs/dam/cfm/components/contentfragment]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Startingwithgod Core Components"
                    sling:resourceType="wcm/core/components/policy/policy">
                <cq:authoring jcr:primaryType="nt:unstructured">
                    <assetToComponentMapping jcr:primaryType="nt:unstructured">
                        <image
                            jcr:primaryType="nt:unstructured"
                            assetGroup="media"
                            assetMimetype="image/*"
                            droptarget="image"
                            resourceType="/apps/CruOrgApp/components/content/image"
                            type="Images"/>
                        <contentfragment
                            jcr:primaryType="nt:unstructured"
                            assetGroup="media"
                            assetMimetype="[text/html,application/vnd.adobe.contentfragment]"
                            droptarget="contentfragment"
                            resourceType="/apps/CruOrgApp/components/content/contentfragment"
                            type="Content Fragment"/>
                        <experiencefragment
                            jcr:primaryType="nt:unstructured"
                            assetGroup="content"
                            assetMimetype="text/html"
                            droptarget="experiencefragment"
                            resourceType="cq/experience-fragments/editor/components/experiencefragment"
                            type="experiencefragment"/>
                        <buildingblock
                            jcr:primaryType="nt:unstructured"
                            assetGroup="block"
                            assetMimetype="text/html"
                            droptarget="buildingblock"
                            resourceType="cq/experience-fragments/editor/components/buildingblock"/>
                    </assetToComponentMapping>
                </cq:authoring>
                <jcr:content
                        jcr:primaryType="nt:unstructured"
                        mergeList="{Boolean}true"/>
            </experience-fragments>
        </core>
    </startingwithgod>
</jcr:root>
